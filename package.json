{
  "name": "montas-frontent",
  "version": "1.0.0",
  "private": true,
  "description": "montas-frontent",
  "author": "JulioMontas",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:css\" \"react-scripts start\"",
    "start:css": "tailwindcss -o src/tailwind.css --watch",
    "build": "npm run build:css && react-scripts build",
    "build:css": "NODE_ENV=production tailwindcss -o src/tailwind.css -m",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.3",
    "@mdx-js/react": "^2.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "gatsby": "^4.23.0",
    "gatsby-plugin-google-analytics": "^4.23.0",
    "gatsby-plugin-image": "^2.23.0",
    "gatsby-plugin-manifest": "^4.23.0",
    "gatsby-plugin-mdx": "^4.2.0",
    "gatsby-plugin-react-helmet": "^5.23.0",
    "gatsby-plugin-sharp": "^4.23.0",
    "gatsby-plugin-sitemap": "^5.23.0",
    "gatsby-source-filesystem": "^4.23.0",
    "gatsby-transformer-sharp": "^4.23.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "gatsby-plugin-postcss": "^5.23.0",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8"
  }
}
